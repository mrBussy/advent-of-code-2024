{
    "name": "rust-devcontainer",
    "image": "mcr.microsoft.com/devcontainers/rust:latest",
    "postCreateCommand": "rustc --version",
    "remoteUser": "vscode",
    "customizations": {
        "vscode": {
            "extensions": [
                "github.vscode-github-actions",
                "EditorConfig.EditorConfig"
            ]
        }
    }
}
